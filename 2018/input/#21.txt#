#ip 1
seti 123 0 4
bani 4 456 4
eqri 4 72 4
addr 4 1 1
seti 0 0 1
seti 0 2 4
bori 4 65536 3     <- #6
seti 10552971 1 4
bani 3 255 5       <- #8
addr 4 5 4
bani 4 16777215 4
muli 4 65899 4
bani 4 16777215 4
gtir 256 3 5 \
addr 5 1 1   | if 256 > [3] then goto #28
addi 1 1 1   |
seti 27 7 1  /
seti 0 1 5
addi 5 1 2
muli 2 256 2
gtrr 2 3 2   \
addr 2 1 1   | if [2] > [3] then goto #26
addi 1 1 1   |
seti 25 0 1  /
addi 5 1 5
seti 17 2 1
setr 5 7 3 <- #26
seti 7 8 1   -> #8
eqrr 4 0 5 <- #28  \
addr 5 1 1         | if [4] == [0] then halt, else goto #6
seti 5 0 1 -> #6   / 
